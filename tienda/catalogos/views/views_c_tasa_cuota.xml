    <record model='ir.ui.view' id='catalogos_tasa_cuota_tree'>
      <field name='name'>catalogos.tasa_cuota.tree</field>
      <field name='model'>catalogos.tasa_cuota</field>
      <field name='arch' type='xml'>
        <tree>
           <field name='rango_o_fijo'/>
           <field name='valor_minimo'/>
           <field name='valor_maximo'/>
           <field name='impuesto'/>
           <field name='factor'/>
           <field name='traslado'/>
           <field name='retencion'/>
           <field name='fecha_inicio_vigencia'/>
           <field name='fecha_fin_vigencia'/>
        </tree>
