              <field name='rango_o_fijo'/>
              <field name='valor_minimo'/>
              <field name='valor_maximo'/>
              <field name='impuesto'/>
              <field name='factor'/>
              <field name='traslado'/>
              <field name='retencion'/>
              <field name='fecha_inicio_vigencia'/>
              <field name='fecha_fin_vigencia'/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
      
      <record id="action_catalogos_tasa_cuota" model="ir.actions.act_window">
        <field name="name">Catálogo de tasas o cuotas de impuestos.</field>
        <field name="res_model">catalogos.tasa_cuota</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
      </record>

      <menuitem id="catalogos_menu_tasas_o_cuotas_de_impuestos" name="Catálogo de tasas o cuotas de impuestos.
" parent="catalogos_menu_principal" action="action_catalogos_tasa_cuota"/>
  </data>
</odoo>